{
    "python.languageServer": "Pylance",
    "python.analysis.exclude": ["**"],
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "django-html": "html",
    },
    "files.associations": {
        "**/*.html" : "html",
        "**/templates/**/*.html":"django-html",
        "**/templates/**/*":"django-txt",
        "**/requirements{/**,*}.{txt,in}":"pip-requirements"
    },
    "beautify.language": {
        "html": [
            "htm",
            "html",
            "django-html"
        ]
    },
}